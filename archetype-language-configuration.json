{
    "comments": {
        "lineComment": "//",
        "blockComment": [ "/*", "*/" ]
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["[%", "%]"],
        ["(", ")"],
        ["do", "done"],
        ["begin", "end"],
        ["match", "end"],
        ["match_detach", "end"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["[%", "%]"],
        ["(", ")"],
        ["\"", "\""],
        ["do", "done"],
        ["begin", "end"],
        ["match", "end"],
        ["match_detach", "end"]
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["[%", "%]"],
        ["(", ")"],
        ["\"", "\""],
        ["do", "done"],
        ["begin", "end"],
        ["match", "end"],
        ["match_detach", "end"]
    ],
    "wordPattern": {
        "pattern": "[A-Za-z_'`][A-Za-z_'.0-9]*"
    }
}
