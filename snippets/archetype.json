{
  "Block": {
    "prefix": "begin",
    "body": [
      "begin $0",
      "end"
    ]
  },
  "IfThen": {
    "prefix": "if",
    "body": [
      "if $0 then"
    ]
  },
  "For": {
    "prefix": "for",
    "body": [
      "for $0 in do",
      "done"
    ]
  },
  "Iter": {
    "prefix": "iter",
    "body": [
      "iter $0 from to do",
      "done"
    ]
  },
  "Match": {
    "prefix": "match",
    "body": [
      "match $0 with",
      "| -> ",
      "end"
    ]
  }
}
