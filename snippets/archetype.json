{
  "Asset": {
    "prefix": "asset",
    "body": [
      "asset ${1:ident} identified by ${2:id} = {",
      "\t${2:id} : int",
      "}"
    ],
    "description": "Asset"
  },
  "Enum": {
    "prefix": "enum",
    "body": [
      "enum ${1:ident} =",
      "\t| $0"
    ],
    "description": "Enum"
  },
  "States": {
    "prefix": "states",
    "body": [
      "states =",
      "\t| Created initial",
      "\t| Aborted",
      "\t| Confirmed",
      "\t| Canceled",
      "\t| Completed"
    ],
    "description": "States"
  },
  "Variable": {
    "prefix": "var",
    "body": [
      "variable ${0:ident} ${1:type} = ${2:value}"
    ],
    "description": "States"
  },
  "Constant": {
    "prefix": "cst",
    "body": [
      "constant ${0:ident} ${1:type} = ${2:value}"
    ],
    "description": "States"
  },
  "Action_Transaction_with_verification": {
    "prefix": "action",
    "body": [
      "action ${1:ident} = {",
      "\tcalled by ${2:role}",
      "\tverification {",
      "\t\tspecification {",
      "\t\t\ts1 : true",
      "\t\t}",
      "\t}",
      "\teffect {",
      "\t\t${0:instructions}",
      "\t}",
      "}"
    ],
    "description": "Action transaction"
  },
  "Action_Transaction": {
    "prefix": "action",
    "body": [
      "action ${1:ident} = {",
      "\tcalled by ${2:role}",
      "\teffect {",
      "\t\t${0:instructions}",
      "\t}",
      "}"
    ],
    "description": "Action transaction"
  },
  "Transition_Transaction": {
    "prefix": "trans",
    "body": [
      "transition ${1:ident} from ${2:source} = {",
      "\tcalled by ${4:role}",
      "\tto ${3:target}",
      "}"
    ],
    "description": "Transition transaction"
  }
}